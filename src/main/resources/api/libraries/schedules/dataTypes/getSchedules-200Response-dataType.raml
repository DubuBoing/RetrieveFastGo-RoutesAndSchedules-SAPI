#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    departureDateTime: 
      type: string
      example: "2019-04-30T16:41:41"
      required: true
    fromLocation:
      type: string
      example: "KLG-MY"
      required: true
    toLocation:
      type: string
      example: "KLG-MY"
      required: true
    availableSeats:
      type: number
      example: 20
      required: true